tabby_cat = "\tI'm tabbed in." # backslash t tabs you in
persian_cat = "I'm split\non a line" # backslash n puts you on a different line.
backslash_cat = "I'm \\ a \\ cat." #double backslash will allow you to print backslash

fat_cat = """
I'll do a list:
\t* Cat food
\t* Fishies
\t* Catnip\n\t* Grass
"""

print tabby_cat
print persian_cat
print backslash_cat
print fat_cat